\n}\r\n\r\n.top-page{\r\n    margin-top: 5rem;\r\n    display: flex;\r\n   flex-direction: column;\r\n  grid-area: top;\r\n}\r\np{\r\n    text-align: left;\r\n}\r\n#experience-code h3 i,\r\n#experience-database h3 i,\r\n#experience-web h3 i,\r\n#experience-design h3 i {\r\n  font-size: 1.2em;\r\n  margin-right: 10px;\r\n  color: #00ADEE;\r\n}\r\n\r\n\r\nh1 {\r\n    font-size: 2em;\r\n}\r\n\r\nh1#headline {\r\n    font-size: 4em;\r\n    margin-top: 0.15em;\r\n    margin-bottom: 0.15em;\r\n    color: #505355;\r\n    border-bottom: solid 1px black;\r\n    align-self: center;\r\n}\r\n\r\nh2 {\r\n    font-size: 1.8em;\r\n    line-height: 1.45em;\r\n}\r\n\r\nh3 {\r\n    font-size: 1em;\r\n    color: #00ADEE;\r\n}\r\n\r\nheader img {\r\n    height: 25px;\r\n}\r\n\r\n.italic-text {\r\n    font-style: italic;\r\n}\r\n\r\n\r\n#main-grid{\r\n    padding: 40px;\r\n    margin-left: 20px;\r\n  display: grid;\r\n    column-gap: 50px;\r\n    color: #ffffff; \r\n    font-size: 1.5rem;\r\n    font-weight: bold;\r\n    text-shadow: 2px 2px 2px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n#experience-grid{\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\n#experience-code{\r\n    display: flex;\r\n    flex-direction: column;\r\n    font-size: 5rem;\r\n}\r\n#experience-database {\r\n    display: flex;\r\n    flex-direction: column;\r\n    font-size: 5rem;\r\n}\r\n#experience-web{\r\n    display: flex;\r\n    flex-direction: column;\r\n    font-size: 5rem;\r\n\r\n}\r\n\r\n#experience-design{\r\n    display: flex;\r\n    flex-direction: column;\r\n    font-size: 5rem;\r\n}\r\n\r\n\r\n    @media screen and (max-width: 450px) {\r\n        #main-grid{\r\n            grid-template-columns: 1fr;\r\n            grid-template-areas: \r\n            \"headline\"\r\n            \"picture\"\r\n            \"contact\"\r\n            \"quote\"\r\n            \"profile\"\r\n            \"experience-grid\";\r\n            grid-template-rows: 1fr 1fr 1fr 1fr 1fr 1fr;\r\n            align-items: center